  //     }
  //   },
  //   "rules": {
  //     "no-debugger": "off"
  //   }
  // },
  "explorer.openEditors.visible": 0,
  // "workbench.editor.limit.perEditorGroup": false,
  // "debug.javascript.codelens.npmScripts": "never",
  "npm.runSilent": true,
  "editor.smoothScrolling": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // "eslint.packageManager": "npm",
  // "npm.packageManager": "npm",
  // "prettier.bracketSpacing": false,
  "prettier.proseWrap": "always",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "python.pythonPath": "/usr/bin/python3",
  // "python.analysis.diagnosticSeverityOverrides": {
  //   "reportMissingImports": "none"
  // },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.fontSize": 16.5,
  "terminal.integrated.letterSpacing": 0.1,
  "terminal.external.linuxExec": "GNOME Terminal",
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "auto-rename-tag.activationOnLanguage": ["*"],
  "window.titleBarStyle": "custom",
  "window.title": "â€ƒ",
  // "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Sorcerer",
  "css.lint.duplicateProperties": "warning",
  "go.toolsManagement.autoUpdate": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.javascript.usePreview": false,
  "projectManager.git.baseFolders": ["/home/garv/Projects"],
  "projectManager.any.baseFolders": ["/home/garv/Projects"],
  "projectManager.any.maxDepthRecursion": 2,
  "projectManager.any.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".git",
    ".expo",
    ".vscode",
    ".github",
    ".expo-shared"
  ],
  "eslint.validate": [
    "mdx",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue"
  ],
  "errorLens.excludePatterns": [
    "File is a CommonJS module"
  ],
  "prettier.requireConfig": true,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "editor.cursorBlinking": "phase",
  "ActiveFileInStatusBar.fullpath": false
}
