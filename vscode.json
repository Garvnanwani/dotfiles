{
  "javascript.suggest.autoImports": true,

  "terminal.integrated.localEchoLatencyThreshold": 0,

  "typescript.suggest.autoImports": true,

  "javascript.updateImportsOnFileMove.enabled": "always",

  "typescript.updateImportsOnFileMove.enabled": "always",

  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },

  "workbench.colorCustomizations": {
    "activityBar.inactiveForeground": "#8990a099",
    "activityBar.foreground": "#b3b9c5cc",
    "foreground": "#d8dce6",
    "icon.foreground": "#d8dce6",
    "editorLineNumber.foreground": "#363d45",
    "activityBar.activeBorder": "#f3f3f3",
    "tab.activeBorder": "#007777",
    "terminal.foreground": "#c7c7c7",
    "terminalCursor.background": "#ffc600",
    "terminalCursor.foreground": "#ffc600",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#1478DB",
    "terminal.ansiBrightBlack": "#808080",
    "terminal.ansiBrightBlue": "#1478DB",
    "terminal.ansiBrightCyan": "#00ffff",
    "terminal.ansiBrightGreen": "#33ff00",
    "terminal.ansiBrightPurple": "#cc00ff",
    "terminal.ansiBrightRed": "#ff0000",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#ffff00",
    "terminal.ansiCyan": "#00c5c7",
    "terminal.ansiGreen": "#3AD900",
    "terminal.ansiPurple": "#ff2c70",
    "terminal.ansiRed": "#ff2600",
    "terminal.ansiWhite": "#c7c7c7",
    "terminal.ansiYellow": "#ffc600"
  },

  "liveServer.settings.donotShowInfoMsg": true,

  "editor.mouseWheelZoom": true,

  "editor.occurrencesHighlight": false,

  "git.autofetch": true,

  "workbench.statusBar.visible": true,

  "workbench.editor.enablePreview": false,

  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },

  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "explorer.sortOrder": "type",

  "editor.letterSpacing": 0.5,

  "workbench.editor.tabSizing": "shrink",

  "html.format.enable": true,

  "html.format.preserveNewLines": true,

  "files.trimFinalNewlines": true,

  "editor.formatOnPaste": true,

  "editor.formatOnSave": true,

  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },

  "go.testOnSave": true,

  "go.lintOnSave": "file",

  "go.formatTool": "goimports",

  "go.testFlags": ["-v"],

  "go.autocompleteUnimportedPackages": true,

  "[go]": {
    "editor.defaultFormatter": "golang.go",

    "editor.insertSpaces": false,

    "editor.formatOnSave": true,

    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },

  "workbench.editor.scrollToSwitchTabs": true,

  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },

  "liveServer.settings.donotVerifyTags": true,

  "editor.suggestSelection": "first",

  // "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",

  "python.languageServer": "Pylance",

  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },

  "editor.fontSize": 16,

  "breadcrumbs.enabled": false,

  "git.enableSmartCommit": true,

  "prettier.semi": false,

  "explorer.confirmDragAndDrop": false,

  "editor.cursorSmoothCaretAnimation": true,

  "workbench.iconTheme": "material-icon-theme",
  "http.systemCertificates": false,
  "explorer.confirmDelete": false,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "git.confirmSync": false,
  "tabnine.experimentalAutoImports": true,

  "editor.lineNumbers": "on",
  "editor.fontFamily": "MonoLisa,MesloLGS NF,Fira Code,Cascadia Mono PL ExtraLight",
  "editor.fontLigatures": true,
  "explorer.autoReveal": false,
  "editor.renderIndentGuides": false,
  "editor.hover.enabled": true,
  "explorer.compactFolders": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.tabCompletion": "on",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.showFoldingControls": "mouseover",
  "editor.find.seedSearchStringFromSelection": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 10,
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "extensions.ignoreRecommendations": false,
  "editor.wordBasedSuggestions": false,
  "files.exclude": {
    "**/.cache": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.next": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/dist": true
  },
  // "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.renderControlCharacters": false,
  "terminal.integrated.rendererType": "dom",
  "eslint.alwaysShowStatus": true,
  "editor.quickSuggestionsDelay": 0,
  "editor.renderWhitespace": "all",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.wordWrap": "on",
  // "debug.toolBarLocation": "docked",
  "window.restoreFullscreen": true,
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "workbench.startupEditor": "newUntitledFile",
  "editor.lightbulb.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  // "editor.folding": false,
  // "javascript.validate.enable": false,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/coverage": true,
    "**/dist": true,
    "**/build": true,
    "**/.build": true,
    "**/.gh-pages": true
  },
  "editor.codeActionsOnSave": {
    // "source.fixAll.eslint": false,
    "source.organizeImports": true
  },
  // "eslint.validate": [
  //   "javascript",
  //   "javascriptreact",
